{
  "name": "rnix/traveller",
  "description": "A traveller who computes average destination",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Roman Nix"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "8.3.4",
    "squizlabs/php_codesniffer": "^3.4"
  },
  "scripts": {
    "phpcs": "phpcs -- --standard=PSR12 src tests",
    "tests": "phpunit ./tests"
  },
  "autoload": {
    "psr-4": {
      "Rnix\\Traveller\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rnix\\Traveller\\Tests\\": "tests/"
    }
  }
}
